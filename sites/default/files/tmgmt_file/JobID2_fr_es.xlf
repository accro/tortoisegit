   <group id="19">
    <note>Tartare de boeuf</note>
    <trans-unit id="19][body][0][value" resname="19][body][0][value">
     <source xml:lang="fr">&lt;p&gt;Tartare de boeuf au couteau. Appelé aussi steak tartare ou filet américain.&lt;/p&gt;&lt;p&gt;Le tartare de boeuf peut être présenté de deux façons : en présentant sur assiette les éléments séparément (comme sur la photo), c'est ce que j'appelle la présentation en kit ! Mais on peut aussi mélanger les ingrédients comme je vous propose de le faire ici. Vous remarquerez que le jaune d'oeuf est posé directement sur la viande et non dans sa coquille comme cela vous est montré la plupart du temps.&lt;br&gt;Temps de préparation : &lt;span class=&quot;duration&quot;&gt;15 minutes&lt;/span&gt;&lt;/p&gt;</source>
     <target xml:lang="es"/>
     <note>Présentation de la recette</note>
    </trans-unit>
    <trans-unit id="19][field_ingredients][0][value" resname="19][field_ingredients][0][value">
     <source xml:lang="fr">400 à 500 g de filet de boeuf (vous pouvez également utiliser toutes les pièces de boeuf tendres) </source>
     <target xml:lang="es"/>
     <note>Ingrédients</note>
    </trans-unit>
    <trans-unit id="19][field_ingredients][1][value" resname="19][field_ingredients][1][value">
     <source xml:lang="fr">4 échalotes ciselées (ou oignons blancs)</source>
     <target xml:lang="es"/>
     <note>Ingrédients</note>
    </trans-unit>
    <trans-unit id="19][field_ingredients][2][value" resname="19][field_ingredients][2][value">
     <source xml:lang="fr">4 cuillers à soupe de câpres</source>
     <target xml:lang="es"/>
     <note>Ingrédients</note>
    </trans-unit>
    <trans-unit id="19][field_ingredients][3][value" resname="19][field_ingredients][3][value">
     <source xml:lang="fr">1/2 bouquet de persil plat</source>
     <target xml:lang="es"/>
     <note>Ingrédients</note>
    </trans-unit>
    <trans-unit id="19][field_ingredients][4][value" resname="19][field_ingredients][4][value">
     <source xml:lang="fr">10 cornichons</source>
     <target xml:lang="es"/>
     <note>Ingrédients</note>
    </trans-unit>
    <trans-unit id="19][field_ingredients][5][value" resname="19][field_ingredients][5][value">
     <source xml:lang="fr">4 jaunes d'oeufs</source>
     <target xml:lang="es"/>
     <note>Ingrédients</note>
    </trans-unit>
    <trans-unit id="19][field_ingredients][6][value" resname="19][field_ingredients][6][value">
     <source xml:lang="fr">4 cuillers d'huile de tournesol</source>
     <target xml:lang="es"/>
     <note>Ingrédients</note>
    </trans-unit>
    <trans-unit id="19][field_ingredients][7][value" resname="19][field_ingredients][7][value">
     <source xml:lang="fr">2 cuillers à soupe de worcestershire sauce</source>
     <target xml:lang="es"/>
     <note>Ingrédients</note>
    </trans-unit>
    <trans-unit id="19][field_ingredients][8][value" resname="19][field_ingredients][8][value">
     <source xml:lang="fr"> 2 cuillers à soupe de moutarde</source>
     <target xml:lang="es"/>
     <note>Ingrédients</note>
    </trans-unit>
    <trans-unit id="19][field_ingredients][9][value" resname="19][field_ingredients][9][value">
     <source xml:lang="fr">Quelques gouttes de tabasco</source>
     <target xml:lang="es"/>
     <note>Ingrédients</note>
    </trans-unit>
    <trans-unit id="19][field_ingredients][10][value" resname="19][field_ingredients][10][value">
     <source xml:lang="fr">sel et poivre du moulinl</source>
     <target xml:lang="es"/>
     <note>Ingrédients</note>
    </trans-unit>
    <trans-unit id="19][field_etapes][0][value" resname="19][field_etapes][0][value">
     <source xml:lang="fr">Hachez la viande au couteau.</source>
     <target xml:lang="es"/>
     <note>Etapes</note>
    </trans-unit>
    <trans-unit id="19][field_etapes][1][value" resname="19][field_etapes][1][value">
     <source xml:lang="fr">Ciseler les oignons (ou échalotes). </source>
     <target xml:lang="es"/>
     <note>Etapes</note>
    </trans-unit>
    <trans-unit id="19][field_etapes][2][value" resname="19][field_etapes][2][value">
     <source xml:lang="fr">Hachez le persil.</source>
     <target xml:lang="es"/>
     <note>Etapes</note>
    </trans-unit>
    <trans-unit id="19][field_etapes][3][value" resname="19][field_etapes][3][value">
     <source xml:lang="fr">Concasser les câpres et les cornichons.</source>
     <target xml:lang="es"/>
     <note>Etapes</note>
    </trans-unit>
    <trans-unit id="19][field_etapes][4][value" resname="19][field_etapes][4][value">
     <source xml:lang="fr">Clarifier les œufs (séparez les jaunes des blancs).</source>
     <target xml:lang="es"/>
     <note>Etapes</note>
    </trans-unit>
    <trans-unit id="19][field_etapes][5][value" resname="19][field_etapes][5][value">
     <source xml:lang="fr">Dans un saladier, mélanger les jaunes d'oeufs avec la moutarde ajoutez la Worcestershire sauce puis l'huile, comme pour une mayonnaise.</source>
     <target xml:lang="es"/>
     <note>Etapes</note>
    </trans-unit>
    <trans-unit id="19][field_etapes][6][value" resname="19][field_etapes][6][value">
     <source xml:lang="fr">Incorporez ensuite les échalotes (ou oignons), le persil, les câpres et les cornichons avec la viande.</source>
     <target xml:lang="es"/>
     <note>Etapes</note>
    </trans-unit>
    <trans-unit id="19][field_etapes][7][value" resname="19][field_etapes][7][value">
     <source xml:lang="fr">Mélanger puis assaisonner avec le sel, le poivre et le Tabasco.</source>
     <target xml:lang="es"/>
     <note>Etapes</note>
    </trans-unit>
    <trans-unit id="19][field_etapes][8][value" resname="19][field_etapes][8][value">
     <source xml:lang="fr">Servir le tartare de boeuf sur assiette. Pour une présentation propre vous pouvez utiliser un cercle pour le dressage.</source>
     <target xml:lang="es"/>
     <note>Etapes</note>
    </trans-unit>
    <trans-unit id="19][field_etapes][9][value" resname="19][field_etapes][9][value">
     <source xml:lang="fr">Accompagnement : pommes de terre frites et salade. Incontournable ! Une fois !</source>
     <target xml:lang="es"/>
     <note>Etapes</note>
    </trans-unit>
    <trans-unit id="19][node_title" resname="19][node_title">
     <source xml:lang="fr">Tartare de boeuf</source>
     <target xml:lang="es"/>
     <note>Titre de la recette</note>
    </trans-unit>
